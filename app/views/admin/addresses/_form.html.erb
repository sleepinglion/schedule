<%= form_with(model: [:admin, address], local: true) do |form| %>
  <div class="card">
    <div class="card-body">
      <div class="form-row">
        <div class="col-12 form-group">
          <%= form.label :address_category_id %>
          <%= form.select :address_category_id,options_from_collection_for_select(AddressCategory.where(:enable=>true).all, 'id', 'title',address.address_category_id),{},{:class=>"form-control"} %>
        </div>
        <div class="col-12 form-group">
          <%= form.label :title %>
          <%= form.text_field :title, :class => 'form-control' %>
        </div>
      </div>
    </div>
  </div>
  <div class="form-group">
    <%= form.submit :class => 'btn btn-primary btn-block btn-lg' %>
  </div>
<% end %>
